<?xml version="1.0"?>
<ContributionSign>
   <![CDATA[
    <div id="HPrint">
    <div class="top" style="text-align: center"><font size="6">${审稿单}$</font></div>
        <span style="font-size:12pt;">稿件编号：${稿件编号}$</span>
        <table  class="mainTable" cellpadding="0" cellspacing="1" align="center" width="100%">
            <tr>
                <td class="left">投稿日期：</td>
                <td class="right">${投稿日期}$</td>
                <td class="left_m">期刊栏目：</td>
                <td class="right_m">${期刊栏目}$</td>
            </tr>
            <tr>
                <td class="left">中文标题：</td>
                <td class="right" colspan="3">${中文标题}$</td>
            </tr>
            <tr>
                <td class="left">英文标题：</td>
                <td class="right" colspan="3">${英文标题}$</td>
            </tr>
            <tr>
                <td class="left">关键词：</td>
                <td class="right" colspan="3">${关键词}$</td>
            </tr>
            <tr>
                <td class="left">作者：</td>
                <td class="right">${作者}$</td>
                <td class="left_m">联系人：</td>
                <td class="right_m">${联系人}$</td>
            </tr>
            <tr>
                <td class="left">联系人电子邮件：</td>
                <td class="right">${联系人电子邮件}$</td>
                <td class="left_m">联系人电话：</td>
                <td class="right_m">${联系人电话}$</td>
            </tr>
            <tr>
                <td class="left">联系人地址：</td>
                <td class="right" colspan="3">${联系人地址}$</td>
            </tr>
            <tr>
                <td class="left">联系人邮编：</td>
                <td class="right">${联系人邮编}$</td>
                <td class="left_m">联系人单位：</td>
                <td class="right_m">${联系人单位}$</td>
            </tr>
            <tr>
                <td class="left">资金资助：</td>
                <td class="right" colspan="3"></td>
            </tr>
            
            <tr style="height:100px;">
                <td class="left">备注：</td>
                <td class="right_r" colspan="3"></td>
            </tr>            
            <tr style="height:300px;">
                <td class="left">审稿情况：</td>
                <td class="right_b" colspan="3"></td>
            </tr>            
            <tr style="height:100px;">
                <td class="left" rowspan="3">出版情况：</td>
                <td class="right_r" colspan="3" ></td>
            </tr>
            

        </table>
    </div>
  ]]>
</ContributionSign>

