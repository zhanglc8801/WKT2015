<div id="divLayout">
    <div position="left" title="审稿状态设置">
        <div id="notice_container" class="notice notice0">
	        <div class="c">
                <a href="javascript:void(0)" id="noticeTip" style="line-height:21px;">系统提示：<br />1.鼠标点击要修改删除的行即可进行对应的编辑和删除操作.<br />2.点击状态图标可修改审稿状态的状态.</a>
            </div>
        </div>
        <div>
            <div class="act-bar" id="divToolbar"></div>
	        <div id="divFlowStatusList"></div>
        </div>
    </div>
    <div position="center" id="divActionContainer" title="审稿状态操作设置">
        <div id="notice_container" class="notice notice0">
	        <div class="c">
                <a href="javascript:void(0)" id="noticeTip" style="line-height:21px;">系统提示：请先选择<b>左侧</b>的审稿状态，对该状态下的操作进行设置。</a>
            </div>
            <div class="c" id="divStatusTitle" style="display:none;">
                <a href="javascript:void(0)" style="line-height:21px;">当前选择状态：<b id="bStatusTitle" class="greenbold"></b></a>
            </div>
        </div>
         <div>
            <div class="act-bar" id="divActionToolbar"></div>
	        <div id="divFlowActionList"></div>
        </div>
    </div>
</div>
@section Scripts{
<script type="text/javascript">
var RootPath = '@Html.RootPath()';
</script>
<script src="@Html.RootPath()/Scripts/page/page.flowset.index.js" type="text/javascript"></script>
